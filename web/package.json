{
  "name": "img2tempdecal_web",
  "engines": {
    "pnpm": ">=7"
  },
  "private": true,
  "version": "1.1.2",
  "type": "module",
  "files": [
    "dist"
  ],
  "scripts": {
    "cargo-build": "cargo build --manifest-path ../wasm/Cargo.toml --target wasm32-unknown-unknown",
    "wasm-bindgen": "wasm-bindgen --target web --out-dir .",
    "dev": "pnpm run cargo-build --features console_error_panic_hook && pnpm run wasm-bindgen ../wasm/target/wasm32-unknown-unknown/debug/img2tempdecal_web.wasm && vite --open",
    "build": "pnpm run cargo-build -r && pnpm run wasm-bindgen ../wasm/target/wasm32-unknown-unknown/release/img2tempdecal_web.wasm && tsc && vite build"
  },
  "dependencies": {
    "@spectrum-web-components/dialog": "^0.31.0",
    "@spectrum-web-components/dropzone": "^0.31.0",
    "@spectrum-web-components/icons-workflow": "^0.31.0",
    "@spectrum-web-components/illustrated-message": "^0.31.0",
    "@spectrum-web-components/link": "^0.31.0",
    "@spectrum-web-components/overlay": "^0.31.0",
    "@spectrum-web-components/popover": "^0.31.0",
    "@spectrum-web-components/switch": "^0.31.0",
    "@spectrum-web-components/theme": "^0.31.0",
    "@spectrum-web-components/toast": "^0.31.0"
  },
  "devDependencies": {
    "prettier": "^2.8.8",
    "typescript": "^5.0.4",
    "vite": "^4.3.8",
    "vite-plugin-pwa": "^0.16.4"
  }
}
